  "types": "./declarations",
  "scripts": {
    "cleanDebug": "rm declarations/debug.d.ts && rm debug.js",
    "cleanDom": "rm declarations/dom.d.ts && rm dom.js",
    "cleanLoader": "rm declarations/loader.d.ts && rm loader.js",
    "cleanMath": "rm declarations/math.d.ts && rm math.js",
    "cleanRaf": "rm declarations/raf.d.ts && rm raf.js",
